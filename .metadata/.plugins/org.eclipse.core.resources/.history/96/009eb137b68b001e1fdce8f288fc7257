package com.dsa.doublyLinkedList;

public class MyApp {

	public static void main(String[] args) {
		DoublyLinkedListImpl db=new DoublyLinkedListImpl();
		System.out.println("Adding At front");
		db.addAtFront(10);
		System.out.println("Added f");
		db.addAtFront(20);
		System.out.println("Added f");
		db.addAtFront(40);
		System.out.println("Added f");
		
		//Adding at rear
		System.out.println("Adding rear");
		db.addRear(50);
		System.out.println("Added r");
		db.addRear(60);
		System.out.println("Added r");
		db.addRear(70);
		System.out.println("Added r");
		
		//Deleting at front
		db.deleteFirst();
		
		System.out.println("Forward Printing");
		db.printForward();
		System.out.println();
		System.out.println("BackWard Printing");
		db.printBackward();
		
	}

}
